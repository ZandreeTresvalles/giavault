{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./component",
        "./component/*"
      ],
      "@containers/*": [
        "./container",
        "./container/*"
      ],
      "@constants/*": [
        "./constants",
        "./constants/*"
      ],
      "@helper/*": [
        "./helper",
        "./helper/*"
      ],
      "@libs/*": [
        "./lib",
        "./lib/*"
      ],
      "@models/*": [
        "./models",
        "./models/*"
      ],
      "@typedefs/*": [
        "./typedefs",
        "./typedefs/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "server.ts", "next.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}
